{
   "background": {
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": "images/icon16.png"
   },
   "content_scripts": [ {
      "js": [ "jquery-3.4.1.min.js", "content.js" ],
      "matches": [ "https://*/*","*://*.myshopify.com/admin/*", "https://auspost.com.au/mypost-business/shipping-and-tracking/orders/add/retail" ]
   } ],
   "description": "YUN Import order from Shopify to MyPost Business with a single click.",
   "icons": {
      "128": "images/icon128.png",
      "16": "images/icon16.png",
      "32": "images/icon32.png",
      "48": "images/icon48.png"
   },
   "manifest_version": 2,
   "name": "YUN Shopify to MyPost Business Importer",
   "permissions": [ "tabs" ],
   "version": "1.1"
}
